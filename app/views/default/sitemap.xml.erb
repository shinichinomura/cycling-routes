<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% Article.all.each do |article| %>
  <url>
    <loc><%= article_url(article) %></loc>
  </url>
  <% end %>
  <% Destination.all.each do |dest| %>
  <url>
    <loc><%= destination_url(dest) %></loc>
  </url>
  <% end %>
</urlset>
